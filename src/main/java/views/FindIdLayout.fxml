<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="findIdPage" prefHeight="800.0" prefWidth="1300.0" style="-fx-background-image: url(/resources/tetris.jpg); -fx-background-size: cover;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.FindIdController">
   <children>
      <Pane layoutX="177.0" layoutY="109.0" prefHeight="582.0" prefWidth="947.0" style="-fx-background-color: rgba(0,0,0,0.7); -fx-background-radius: 20px;">
         <children>
            <Label layoutX="385.0" layoutY="14.0" prefHeight="59.0" prefWidth="178.0" text="계정 ID 찾기" textFill="WHITE">
               <font>
                  <Font name="HCR Dotum Bold" size="32.0" />
               </font>
            </Label>
            <TextField fx:id="txtName" layoutX="282.0" layoutY="163.0" prefHeight="59.0" prefWidth="384.0" promptText="이름을 입력해주세요.">
               <font>
                  <Font name="HCR Dotum Bold" size="19.0" />
               </font>
            </TextField>
            <TextField fx:id="txtCheck" layoutX="282.0" layoutY="320.0" prefHeight="59.0" prefWidth="384.0" promptText="본인 확인 질문을 입력하세요.">
               <font>
                  <Font name="HCR Dotum Bold" size="18.0" />
               </font>
            </TextField>
            <ComboBox fx:id="checkBox" layoutX="282.0" layoutY="278.0" prefHeight="42.0" prefWidth="384.0" promptText="본인 확인 질문" />
            <Label fx:id="lblFind" alignment="CENTER" layoutX="263.0" layoutY="450.0" prefHeight="42.0" prefWidth="422.0" textFill="WHITE">
               <font>
                  <Font name="HCR Dotum Bold" size="30.0" />
               </font>
            </Label>
            <Button layoutX="408.0" layoutY="395.0" mnemonicParsing="false" onMouseClicked="#findId" onMouseEntered="#hoverSound" prefHeight="47.0" prefWidth="131.0" style="-fx-background-color: rgba(0,0,0,0);" styleClass="btn" text="확인" textFill="WHITE">
               <font>
                  <Font name="HCR Dotum Bold" size="26.0" />
               </font>
            </Button>
            <Button layoutX="14.0" layoutY="513.0" mnemonicParsing="false" onMouseClicked="#close" onMouseEntered="#hoverSound" prefHeight="55.0" prefWidth="152.0" style="-fx-background-color: rgba(0,0,0,0);" styleClass="redbtn" text="뒤로가기" textFill="WHITE">
               <font>
                  <Font name="HCR Dotum Bold" size="26.0" />
               </font>
            </Button>
            <Label layoutX="282.0" layoutY="104.0" prefHeight="59.0" prefWidth="178.0" text="이름 입력" textFill="WHITE">
               <font>
                  <Font name="HCR Dotum Bold" size="22.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
